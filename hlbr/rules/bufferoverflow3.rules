
# name:       bufferoverflow3.rules
# created by: Joao Eriberto Mota Filho
# date:       14 Dec 05
# update:     none
# target:     block udp buffer overflow attempt attempts.


<rule>
ip dst(servers)
udp dst(13,37,53,123)
udp nocase(AAAAAAAAAAAAAAAAAAAAAAAAAAAAAA)
message=(bufferoverflow3-1) buffer overflow attempt
action=action1
</rule>

