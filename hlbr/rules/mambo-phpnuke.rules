
# nome / name:             mambo-phpnuke.rules
# criado por / created by: Joao Eriberto Mota Filho
# data / date:             26 Dez 05 / 26 Dez 05
# atualizacao / update:    nenhuma / none

# Objetivo / Target: Bloquear requisicoes invalidas / Block invalid requests.

<rule>
ip dst(www)
tcp dst(80)
tcp content(?_REQUEST[option]=com_content&_REQUEST[Itemid]=1&GLOBALS=&mosConfig_absolute_path=http://)
message=(mambo-phpnuke-1) Ataque ao mambo / Mambo attack
action=action1
</rule>

<rule>
ip dst(www)
tcp dst(80)
tcp content(admin_styles.phpadmin_styles.php?phpbb_root_path=http://)
message=(mambo-phpnuke-2) Ataque ao php-nuke / Php-nuke attack
action=action1
</rule>

