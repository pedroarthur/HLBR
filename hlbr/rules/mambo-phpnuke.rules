
# name:       mambo-phpnuke.rules
# created by: Joao Eriberto Mota Filho
# date:       26 Dec 05
# update:     none
# target:     block invalid requests.

<rule>
ip dst(www)
tcp dst(80)
tcp regex(._REQUEST.option..com_content._REQUEST.Itemid..1.GLOBALS..mosConfig_absolute_path.http)
message=(mambo-phpnuke-1-re) mambo attack
action=action1
</rule>

<rule>
ip dst(www)
tcp dst(80)
tcp regex(admin_styles\.phpadmin_styles\.php.phpbb_root_path.http)
message=(mambo-phpnuke-2-re) php-nuke attack
action=action1
</rule>

