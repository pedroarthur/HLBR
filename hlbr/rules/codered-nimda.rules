
# nome / name:             codered-nimda.rules
# criado por / created by: Joao Eriberto Mota Filho
# data / date:             14 Dez 05 / 14 Dec 05
# atualizacao / update:    nenhuma / none

# Objetivo / Target: Bloquear os Worms Code Red e NIMDA / Block the Code Red and NIMDA Worms.

# Referencia / Reference: http://www.mycert.org.my/advisory/MS-035.102001.html

<rule>
ip dst(www)
tcp dst(80)
tcp content(default.ida)
message=(codered_nimda) Tentativa de default.ida / default.ida attempt
action=acao1
</rule>

<rule>
ip dst(www)
tcp dst(80)
tcp content(root.exe)
message=(codered_nimda) Tentativa de root.exe / root.exe attempt
action=acao1
</rule>

<rule>
ip dst(www)
tcp dst(80)
tcp nocase(cmd.exe)
message=(codered_nimda) Tentativa de cmd.exe / cmd.exe attempt
action=acao1
</rule>

<rule>
ip dst(www)
tcp dst(80)
tcp content(explorer.exe)
message=(codered_nimda) Tentativa de explorer.exe / explorer.exe attempt
action=acao1
</rule>
